[
    { "pattern": "\\b(import|export|from|const|let|var|function|return|async|await)\\b", "class": "keyword" },
    { "pattern": "\\b([a-zA-Z_][a-zA-Z0-9_]*)\\b(?=\\()", "class": "method" },
    
    { "pattern": "\"([^\"]*)\"|'([^']*)'", "class": "string" },
    { "pattern": "\\b([0-9]+)\\b", "class": "number" },
    { "pattern": "\\/\\/.*", "class": "comment" },
    { "pattern": "\\/\\*[\\s\\S]*?\\*\\/", "class": "comment" },
    { "pattern": "[{}()\\[\\];,]", "class": "symbol" }
]
